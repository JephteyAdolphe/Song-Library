      <Label fx:id="song_name" layoutX="320.0" layoutY="100.0" />
      <Label fx:id="artist_name" layoutX="320.0" layoutY="156.0" />
      <Label fx:id="album_name" layoutX="320.0" layoutY="207.0" />
